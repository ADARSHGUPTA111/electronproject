{
  "name": "Custard",
  "homepage": "./",
  "version": "0.3.1",
  "description": "One app to rule all communication apps",
  "private": true,
  "author": {
    "name": "Invide Team",
    "email": "hello@invidelabs.com",
    "url": "https://www.invidelabs.com/developer.html"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron": "^9.1.2",
    "electron-is-dev": "^0.3.0",
    "macos-version": "^5.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-new-window": "^0.1.2",
    "react-router-dom": "^4.3.1",
    "styled-components": "^5.1.1"
  },
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder build-c.extraMetadata.main=build/main.js",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "your.id",
    "extraMetadata": {
      "main": "build/main.js"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "assets/icons/icon",
      "target": [
        "nsis"
      ]
    }
  },
  "devDependencies": {
    "electron-process-manager": "^1.0.0",
    "electron-process-reporter": "^1.4.0",
    "wait-on": "^2.1.0",
    "electron-builder": "^20.27.1",
    "react-scripts": "3.4.1",
    "concurrently": "^3.6.1",
    "cross-env": "^7.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
