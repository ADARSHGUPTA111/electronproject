{
  "name": "custard",
  "version": "0.3.1",
  "description": "One app to rule all communication apps",
  "private": true,
  "author": {
    "name": "Invide Team",
    "email": "hello@invidelabs.com",
    "url": "https://www.invidelabs.com/developer.html"
  },
  "dependencies": {
    "cross-env": "6.0.2",
    "electron-is-dev": "1.1.0",
    "electron-log": "^3.0.8",
    "macos-version": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.12.1",
    "react-new-window": "^0.1.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "update-electron-app": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "4.1.2",
    "electron": "^11.1.0",
    "electron-builder": "^21.2.0",
    "prettier": "^1.4.4",
    "react-scripts": "3.1.2",
    "wait-on": "3.3.0"
  },
  "build": {
    "appId": "com.electron.custard",
    "mac": {
      	"identity": null,
	"category": "public.app-category.productivity"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm react-build && electron-builder --publish=always",
    "build": "npm run electron-build",
    "start-electron": "concurrently \"SET BROWSER=none&&npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
